@{
    Layout = "_AuthLayout";
    ViewData.SetTitle(Localizer[Titles.RegisterSuccess]);
    var predefinedViewMessageModel = new PredefinedViewMessageModel("Views/Client/Authentication/RegisterSuccess.cshtml", "https://emeraude.dev/client");
}

<partial name="_EmeraudeClientPredefinedMessage" model="predefinedViewMessageModel" />

<div class="form-message">
    <t element="h5" class="text-center">@Titles.RegisterSuccess</t>
    <t element="p">@Messages.RegistrationSuccessMessage</t>
    <t element="a" href="@Url.IndexAction()" class="btn btn-primary">@ActionsTexts.BackToHomePage</t>
</div>